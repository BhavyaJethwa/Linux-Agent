{
    "family": "linux-agent-backend",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::201132760938:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::201132760938:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "<IMAGE_BACKEND>",
        "essential": true,
        "portMappings": [
          { "containerPort": 8000, "hostPort": 8000, "protocol": "tcp" }
        ],
        "environment": [
            { "name": "OPENAI_API_KEY", "value": "<OPENAI_API_KEY>"}
        ],
        "secrets": [
            {
                "name": "PEM_CONTENT",
                "valueFrom": "arn:aws:secretsmanager:us-east-1:201132760938:secret:linux-agent-server-pem"
            }

        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/linux-agent-backend",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  