{
    "family": "linux-agent-backend",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::201132760938:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "<IMAGE_BACKEND>",
        "essential": true,
        "portMappings": [
          { "containerPort": 8000, "hostPort": 8000, "protocol": "tcp" }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/linux-agent-backend",
            "awslogs-region": "ap-south-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  