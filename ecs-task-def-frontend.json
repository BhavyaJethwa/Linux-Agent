{
    "family": "linux-agent-frontend",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::201132760938:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "<IMAGE_FRONTEND>",
        "essential": true,
        "portMappings": [
          { "containerPort": 8501, "hostPort": 8501, "protocol": "tcp" }
        ],
        "environment": [
            {
                "name": "BACKEND_URL",
                "value": "<BACKEND_URL"
            }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/linux-agent-frontend",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  